                        <Insets right="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="radius" prefHeight="25.0" prefWidth="69.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="31.0" prefWidth="385.0">
               <children>
                  <Label prefHeight="24.0" prefWidth="58.0" text="Iterations">
